<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>latest</LangVersion>
		<Authors>Leandro Ferreira</Authors>
		<Company>VIP Soluções</Company>
		<Description>Simple library for generate sql queries to MS SQL SERVER</Description>
		<Copyright>VIP Soluções</Copyright>
		<Version>1.0.16</Version>
		<PackageTags>sql;querybuilder;sql-server</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<RepositoryUrl>https://github.com/leandrovip/Vip.SqlQuery</RepositoryUrl>
		<PackageProjectUrl>https://github.com/leandrovip/Vip.SqlQuery</PackageProjectUrl>

	</PropertyGroup>
</Project>